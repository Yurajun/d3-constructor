.d3
	display block

#container
	position fixed
	top 0
	right 0
	left 0
	bottom 0

.kit-editor
	position relative
	width 100%
	height 100%

	#svg-editor
		width 100%
		height 100%

svg
	.underlay
		pointer-events all
		fill none

g.axis
	pointer-events none

	line
		fill none
		stroke #ddd
		shape-rendering cripsEdges
		// vector-effect non-scaling-stroke


.svg-border
	// pointer-event none
	stroke #a9b0b8
	stroke-width 1.5
	fill none
